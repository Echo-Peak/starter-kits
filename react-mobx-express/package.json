{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "description": "basic-medium sized react application boilerplate",
  "author": "michael wheeler",
  "scripts": {
    "postinstall": "node scripts/install.js",
    "dev": "npm-run-all --parallel build-webpack-d nodemon-server browser-sync --sequential reload --parallel reload",
    "build-webpack-p": "webpack --config webpack.config.js -p --env.env=production",
    "build-webpack-d": "webpack --config webpack.config.js --env.env=development",
    "app": "node app/server/app.js",
    "nodemon-server": "node scripts/nodemon.js",
    "browser-sync": "node scripts/browser-sync.js",
    "nodemon-stand-alone": "nodemon app/server/app.js",
    "build": "npm-run-all --parallel  build-webpack-p run-app",
    "dev:iso": "npm-run-all --parallel nodemon:iso build-webpack-d browser-sync",
    "app:iso": "node app/server/app.js --production"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "npm-run-all": "^4.0.2",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "browser-sync": "^2.18.8",
    "colors": "^1.1.2",
    "css-loader": "^0.27.3",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta11",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "vorpal": "^1.11.4",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
}
